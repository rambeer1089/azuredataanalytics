{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azuredataanalytics"
		},
		"azuretrainingdatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azuretrainingdatabase'"
		},
		"organizeddlsconnection_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'organizeddlsconnection'"
		},
		"azuretrainingkeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azuredataanalytics.vault.azure.net/"
		},
		"dlsfolderproviderfunction_properties_typeProperties_functionAppUrl": {
			"type": "string",
			"defaultValue": "https://azureproviderdlstargetprovider.azurewebsites.net"
		},
		"organizeddlsconnection_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azuretrainingdls.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azuredataanalytics_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"organizeddlsconnection_accountKey": {
						"value": "[parameters('organizeddlsconnection_accountKey')]"
					},
					"azuretrainingdatabase_connectionString": {
						"value": "[parameters('azuretrainingdatabase_connectionString')]"
					},
					"azuretrainingkeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('azuretrainingkeyvault_properties_typeProperties_baseUrl')]"
					},
					"organizeddlsconnection_properties_typeProperties_url": {
						"value": "[parameters('organizeddlsconnection_properties_typeProperties_url')]"
					},
					"dlsfolderproviderfunction_properties_typeProperties_functionAppUrl": {
						"value": "[parameters('dlsfolderproviderfunction_properties_typeProperties_functionAppUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}